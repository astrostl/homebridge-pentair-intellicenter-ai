{
  "displayName": "Homebridge IntelliCenter",
  "name": "homebridge-intellicenter",
  "version": "2.2.2",
  "description": "Homebridge plugin for integrating Pentair IntelliCenter into HomeKit.",
  "license": "Apache-2.0",
  "repository": {
